{
    "version": 2,
    "functions": {
        "api/*.php": { "runtime": "vercel-php@0.6.2" }
    },
    "routes": [{ 
        "src": "/build/assets/(.*)", "dest": "/public/build/assets/" 
    },
    { 
        "src": "/images/(.*)", "dest": "/public/images/" 
    },
    { 
        "src": "/css/(.*)", "dest": "/public/css/" 
    },
    { 
        "src": "/js/(.*)", "dest": "/public/js/" 
    },
    { 
        "src": "/images/(.*)", "dest": "/public/images/" 
    },
    { 
        "src": "/(.*)", "dest": "/api/index.php" 
    }]

}